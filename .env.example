# Environment Variables for Optical Try-On v5.0

# Analytics Configuration
VITE_ANALYTICS_ENABLED=true
VITE_ANALYTICS_ENDPOINT=https://api.opticaltryon.com/analytics

# Feature Flags
VITE_ENABLE_EAR_DETECTION=true
VITE_ENABLE_ADVANCED_CALIBRATION=true
VITE_ENABLE_FEEDBACK_COLLECTION=true

# Performance Settings
VITE_MAX_FPS=30
VITE_CAMERA_RESOLUTION=high

# API Configuration
VITE_API_BASE_URL=https://api.opticaltryon.com
VITE_API_KEY=your_api_key_here

# Debug Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info

# CDN Configuration
VITE_CDN_URL=https://cdn.opticaltryon.com
VITE_MEDIAPIPE_CDN=https://cdn.jsdelivr.net/npm/@mediapipe
